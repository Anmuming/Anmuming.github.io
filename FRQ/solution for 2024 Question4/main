public class Main {
  public static void main(String[] args) {
    System.out.println("Hello world!");
    int [][] grid ={
      {12,3,4,13,5},
      {11,21,2,14,16},
      {7,8,9,15,0},
      {10,17,20,19,1},
      {18,22,30,25,6},
    };
    Location l1= new Location(0,0,grid);
    l1.getNextLoc(0,0);
    System.out.println(l1.getNextLoc(0,0));
    System.out.println(l1.getNextLoc(1,3));
    System.out.println(l1.getNextLoc(2,4));
    System.out.println(l1.getNextLoc(4,3));


    
  }
